# Ref: https://github.com/codespell-project/codespell#using-a-config-file
[codespell]
skip = .git,*.svg,package-lock.json,*.css,*.min.*,.codespellrc
check-hidden = true
# ignore all acronyms and camel cased words -- ie words with a capitalized letter
ignore-regex = \b([a-zA-Z]*[A-Z][a-zA-Z]*)\b
ignore-words-list = bu,ot,struc
